  "scripts": {
    "start-localstack": "make start-localstack",
    "stop-localstack": "make stop-localstack",
    "generate": "cd .. && make generate",
    "dev": "yarn generate && NODE_ENV=development tsx ./src/server/development.ts",
    "dev:proxied": "yarn generate && NODE_ENV=development VITE_PROXIED=true NODE_TLS_REJECT_UNAUTHORIZED=0 tsx ./src/server/development.ts",
    "build": "NODE_OPTIONS=--max_old_space_size=6144 NODE_ENV=production react-ssr build-app",
    "preview": "NODE_ENV=production node dist/server/production.js",
    "test": "NODE_ENV=test TEST=true jest --maxWorkers=50%",
    "ci:test": "NODE_ENV=test TEST=true jest --collectCoverage --silent --maxWorkers=8",
    "ci:test:codebuild": "NODE_ENV=test TEST=true jest --collectCoverage --silent --maxWorkers=60",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --cache-strategy content .",
    "lint:fix": "npm run lint -- --fix",
    "lint:css": "stylelint src/**/*.css",
    "prettier": "prettier . --check --write",
    "knip": "knip",
    "ci:lint": "NODE_OPTIONS=--max_old_space_size=4000 npm run lint -- --quiet",
    "preinstall": "node -e \"if (process.env.npm_execpath.indexOf('yarn') >= 0) throw new Error('Use npm for installing dependencies. Yarn is not supported for this codebase for now')\"",
    "postinstall": "patch-package",
    "prepare": "cd .. && husky install site/.husky"
  },
  "knip": {
    "entry": [
      "./src/server/development.ts!",
      "./src/server/production.ts!",
      "./src/ssr/entry-client.tsx!",
      "./src/ssr/entry-server.tsx!"
    ],
    "project": [
      "**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}"
    ],
    "ignore": [
      "**/coverage/**",
      "**/dist/**",
      "**/*.test.{js,cjs,mjs,jsx,ts,cts,mts,tsx}"
    ],
    "ignoreDependencies": [
      ".*\\.css"
    ],
    "ignoreExportsUsedInFile": true
  },
